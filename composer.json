{
    "name": "yaliniyalu/drchrono-messenger-bot",
    "description": "Messenger Bot for DrChrono",
    "keywords": [
        "drchrono",
        "messenger-bot"
    ],
    "homepage": "https://github.com/yaliniyalu/drchrono-messenger-bot",
    "license": "MIT",
    "authors": [
        {
            "name": "Samar Yalini",
            "email": "samarjben@outlook.com",
            "homepage": "http://www.yalusoft.com/"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "botman/botman": "^2.6",
        "botman/driver-facebook": "^1.11",
        "guzzlehttp/guzzle": "6.5",
        "kerox/messenger": "^3.4",
        "league/oauth2-client": "^2.6",
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.1",
        "phpfastcache/phpfastcache": "^8.0",
        "robmorgan/phinx": "^0.12.4",
        "slim/psr7": "^1.1",
        "slim/slim": "^4.5",
        "spomky-labs/base64url": "^2.0",
        "symfony/css-selector": "^5.2",
        "symfony/dom-crawler": "^5.2",
        "thingengineer/mysqli-database-class": "dev-master",
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S 127.0.0.1:8088 -t public",
        "test": "phpunit"
    }
}
